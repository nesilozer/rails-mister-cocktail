<div class="banner">
  <h1>Cocktail Library</h1>
</div>
<div class="container">
  <div class="row justify-content-center">
    <div class="card">
      <% @cocktails.each do |cocktail| %>
        <ul>
          <li><%= cocktail.name %></li>
        </ul>
      <% end %>
    </div>
  </div>
</div>




